openDeploymentLog: aPackage 
	| dom |
	dom := IXMLDOMDocument new.
	dom loadURL: 'file://' , aPackage imageStripper logFileName.
	^dom!

openIdeaSpace
	"Create a new IdeaSpace by opening a template file"

