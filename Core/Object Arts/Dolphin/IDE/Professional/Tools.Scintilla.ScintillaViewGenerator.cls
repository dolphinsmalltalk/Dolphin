generateCommandMap
	| stream selector |
	commands addAll: self remappedCommands.
	stream := String writeStream.
	selector := #initializeCommands.
	stream
		display: selector;
		cr;
		nextPutAll: 'self addClassConstant: ''Commands'' value: (IdentityDictionary withAll: {';
		cr.
	commands at: 'SCI_NULL' put: #yourself.
	(self sciCommands asSortedArray copyWith: 'SCI_NULL') do: 
			[:each |
			stream
				display: each;
				nextPutAll: ' -> ';
				print: (commands at: each)]
		separatedBy: 
			[stream
				nextPut: $.;
				cr].
	stream nextPutAll: '})'.
	self
		compile: stream contents
		selector: selector
		in: KeyBinding class
		categories: self autoGenMethodCategories , { 'private'. 'development' }!

generateConstantsPool
	| stream changes source |
	stream := String writeStream: 8192.
	stream
		nextPutAll: #defineConstantsPool;
		cr;
		nextPutAll: '##(UI subclass: #''UI.Scintilla''
	instanceVariableNames: ''''
	classVariableNames: ''''
	imports: #()
	classInstanceVariableNames: ''''
	classConstants: {';
		cr.
	(self constantsPool associations reject: [:each | self class deprecatedConstants includes: each key])
		sort do: 
				[:each |
				stream
					print: each key;
					nextPutAll: ' -> ';
					print: each value;
					nextPut: $.;
					cr].
	stream nextPutAll: '})'.
	changes := model changes.
	source := (Parser parseMethod: stream contents) formattedCode.
	((self class includesSelector: #defineConstantsPool) not
		or: [source ~= (self class sourceCodeAt: #defineConstantsPool)])
			ifTrue: 
				[changes addChangeFirst: (Refactory.Browser.AddMethodChange
							compile: source
							in: self class
							categories: { self autoGenCategoryName. 'development' })]!

generateEventHandler: aScintillaEventDef
	| stream |
	aScintillaEventDef isDeprecated
		ifTrue: 
			[Transcript
				display: 'Ignoring deprecated event ';
				print: aScintillaEventDef constName;
				cr.
			^self].
	(self scintillaViewClass compiledMethodAt: aScintillaEventDef selector ifAbsent: [])
		ifNotNil: 
			[:existingMethod |
			(existingMethod categories includes: self autoGenCategoryName asMethodCategory)
				ifFalse: 
					[Transcript
						display: 'Default handler for ';
						print: aScintillaEventDef selector;
						display: ' not generated due to selector clash';
						cr.
					^self]].
	stream := String writeStream.
	aScintillaEventDef generateHandlerOn: stream.
	self
		compileMethod: aScintillaEventDef selector
		source: stream contents
		categories: self autoGenEventCategories!

generateFunction: func
	| stream |
	func hasReturnValue ifTrue: [defaultReturnValues at: func constName put: func defaultReturnValue].
	((self class ignoredMessages includes: func constName) or: 
			[| existing |
			existing := ScintillaView compiledMethodAt: func selector ifAbsent: nil.
			existing notNil and: [(existing categories includes: self autoGenCategoryName asMethodCategory) not]])
		ifTrue: 
			[Notification signal: 'Ignoring message ' , func constName.
			^self].
	stream := String writeStream: 256.
	func emitMethodTextOn: stream.
	self compileMethod: func selector source: stream contents.
	func isCommand ifTrue: [commands at: func constName put: func selector]!

generateFunctions
	functions do: [:each | self generateFunction: each]!

generateNotificationMap
	| stream scnFirst |
	events isEmpty ifTrue: [^self].
	stream := String writeStream.
	scnFirst := events first constName.
	stream
		display: #nmNotify:;
		nextPutAll: ' pNMHDR
	"Private - Handler for a redirected generic WM_NOTIFY message."';
		cr;
		crtab;
		nextPutAll: '^(##((Array new: ';
		nextPutAll: events last constName;
		nextPutAll: ' - ';
		nextPutAll: scnFirst;
		nextPutAll: ' + 1)';
		crtab: 2.
	events do: 
			[:each |
			| constName |
			constName := each constName.
			stream
				nextPutAll: 'at: (';
				nextPutAll: constName;
				nextPutAll: ' - ';
				nextPutAll: scnFirst;
				nextPutAll: ' + 1) put: ';
				print: (each isDeprecated ifFalse: [each selector]);
				nextPut: $;;
				crtab: 2].
	stream
		nextPutAll: 'yourself) ';
		display: #lookup:;
		nextPutAll: ' (pNMHDR int32AtOffset: NMHDR._OffsetOf_code) - ##(';
		nextPutAll: scnFirst;
		nextPutAll: ' - 1))
		ifNotNil: [:action | self perform: action with: pNMHDR]'.
	self
		compile: stream contents
		selector: #nmNotify:
		in: self scintillaViewClass
		categories: (self autoGenMethodCategories copyWith: 'event handling-win32')!

generateNullLibraryDefaults
	| stream selector |
	self setKnownDefaults.
	stream := String writeStream.
	selector := #returnValues.
	stream
		display: selector;
		cr;
		nextPutAll: '^##((IdentityDictionary withAll: {';
		cr.
	defaultReturnValues associations sort do: 
			[:each |
			| key |
			key := each key.
			(self class deprecatedConstants includes: key) ifTrue: [
				key := constantsPool at: key].
			stream
				display: key;
				nextPutAll: ' -> ';
				display: each value]
		separatedBy: 
			[stream
				nextPut: $.;
				cr].
	stream
		nextPutAll: '})';
		cr;
		display: #beImmutableObject;
		nextPut: $;;
		nextPutAll: 'yourself';
		nextPut: $).
	self
		compile: stream contents
		selector: selector
		in: NullScintillaLibrary
		categories: self autoGenMethodCategories , { 'private' }!

getAndResetCommentQueue
	| answer |
	answer := commentQueue contents.
	commentQueue := WriteStream on: (Utf8String new: 160).
	^answer!

initialize
	events := OrderedCollection new: 256.
	commentQueue := WriteStream on: (Utf8String new: 160).
	lexerConstantPrefixes := Set new.
	previewChanges := true.
	commands := LookupTable new.
	"Special case"
	commands at: 'SCI_SETZOOM' put: #resetZoom.
	parsingDeprecated := false.
	model := Refactory.Browser.RBModel new.
	generatedSelectors := LookupTable new.
	constantsPool := Dictionary new.
	aliases := LookupTable new.
	functions := OrderedCollection new.
	defaultReturnValues := LookupTable new!

isIgnoredConstant: aString
	
	"INDIC_CONTAINER, INDIC_IME, INDIC_IME_MAX, and INDIC_MAX are indicator numbers, not IndicatorStyles so should not really be in the INDIC_ enumeration."
	^(#('INDIC_CONTAINER' 'INDIC_IME' 'INDIC_IME_MAX' 'INDIC_MAX') includes: aString) or: 
			["We don't want to bloat the constants pool with a load of lexer defines that we don't need in Dolphin"
			lexerConstantPrefixes anySatisfy: [:each | aString beginsWith: each]]!

parseAlias: aReadableStream 
	| aliased alias |
	aliased := aReadableStream upTo: $=.
	alias := aReadableStream nextWord.
	aliases at: alias put: aliased
!

parseCategory: aReadableStream 
	| category |
	category := aReadableStream
				skipSeparators;
				upToEnd.
	parsingDeprecated := category asLowercase = 'deprecated'!

parseCommentLine: aStream
	(aStream peekFor: $#)
		ifTrue: 
			[self parseFileComment: aStream.
			^self].
	(aStream peekFor: $!!)
		ifTrue: 
			[self parseShBang: aStream.
			^self].
	aStream skipSeparators.
	^self parseInterfaceComment: aStream!

parseEnum: aReadableStream
	| typeName prefix |
	typeName := aReadableStream upTo: $=.
	prefix := aReadableStream nextWord.
	TypeReference declareEnum: typeName prefix: prefix!

parseEvent: aReadableStream 
	| event |
	event := (EventDefinition tokens: (self splitDefinition: aReadableStream))
				comment: self getAndResetCommentQueue;
				isDeprecated: parsingDeprecated;
				yourself.
	self constantsPool at: event constName put: event id.
	self generateEventHandler: event.
	events add: event!

parseFile
	| filename |
	filename := (FileOpenDialog on: 'scintilla.iface')
				initialDirectory: (File removePathDelimiter: ScintillaView owningPackage path);
				fileTypes: #(#('Scintilla Interface Files (*.iface)' '*.iface') #('All Files (*.*)' '*.*'));
				defaultExtension: 'iface';
				showModal.
	self parseFile: filename!

parseFile: aFileName
	| cFile |
	self defineAdditionalConstants.
	TypeReference resetTypeMap.
	cFile := FileStream read: aFileName.
	[cFile atEnd] whileFalse: [self parseLine: cFile nextLine].
	cFile close.
	self setEnumSpecialDefaultValues.
	self generateFunctions.
	self generateConstantsPool.
	self generateNotificationMap.
	self generateCommandMap.
	self generateNullLibraryDefaults.
	self removeStaleAutoGeneratedMethods.
	self updateLibVersion.
	previewChanges
		ifTrue: [self browseChanges]
		ifFalse: 
			["Apply the changes."
			self applyChanges]!

parseFileComment: aReadableStream 
	"File comment lines are ignored"

	!

parseFunction: aReadableStream type: aString
	| func |
	func := (FunctionDefinition tokens: (self splitDefinition: aReadableStream))
				funcType: aString;
				isDeprecated: parsingDeprecated;
				comment: self getAndResetCommentQueue;
				yourself.
	self constantsPool at: func constName put: func id.
	functions addLast: func!

parseInterfaceComment: aReadableStream 
	commentQueue isEmpty ifFalse: [commentQueue space].
	commentQueue nextPutAll: aReadableStream upToEnd trimBlanks!

parseLex: aReadableStream 
	| lexerName lexerId |
	lexerName := aReadableStream upTo: $=.
	lexerId := aReadableStream nextWord.
	lexerConstantPrefixes addAll: aReadableStream upToEnd subStrings!

parseLine: aString
	| lineStream lineType |
	lineStream := aString readStream.
	(lineStream peekFor: $#)
		ifTrue: 
			[self parseCommentLine: lineStream.
			^self].
	lineType := lineStream nextWord.
	lineType ifNil: [^self].
	lineType = 'cat' ifTrue: [^self parseCategory: lineStream].
	(#('fun' 'get' 'set') includes: lineType) ifTrue: [^self parseFunction: lineStream type: lineType].
	lineType = 'val' ifTrue: [^self parseValue: lineStream].
	lineType = 'evt' ifTrue: [^self parseEvent: lineStream].
	lineType = 'enu' ifTrue: [^self parseEnum: lineStream].
	lineType = 'lex' ifTrue: [^self parseLex: lineStream].
	lineType = 'ali' ifTrue: [^self parseAlias: lineStream].
	lineType = 'enu' ifTrue: [^self parseEnum: lineStream].

	"Catch all"
	self error: 'Unrecognised line: ' , aString!

parseShBang: aReadableStream 
	"You aint gonna need these, even if there were any"

	!

parseValue: aReadableStream
	| value numberStream radix constName |
	self getAndResetCommentQueue.
	constName := aReadableStream upTo: $=.
	value := aReadableStream upToEnd.
	numberStream := value readStream.
	radix := (numberStream skipWhileMatchAll: '0x') ifTrue: [16] ifFalse: [10].
	value := Integer readFrom: numberStream radix: radix.
	numberStream atEnd ifFalse: [^self].
	(self isIgnoredConstant: constName) ifFalse: [self constantsPool at: constName put: value].
	(TypeReference enumForConst: constName) ifNotNil: [:enum | enum addConst: constName value: value]!

previewChanges: aBoolean 
	"Set whether the parser is in preview mode. When in preview mode the generator will not apply the
	changes it generates, but just open a change browser on them instead."

	previewChanges := aBoolean!

