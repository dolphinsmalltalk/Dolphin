!ToolbarButton methodsFor!

publishedAspects
	| aspects bmp |
	aspects := super publishedAspects.
	bmp := self bitmap.
	(bmp isNil or: [bmp isInteger]) 
		ifFalse: 
			[aspects add: (Aspect choice: #bitmapIndex
						from: (0 to: bmp extent x // (self toolbar ifNil: [16] ifNotNil: [:tb | tb buttonSize x])))].
	^aspects! !
