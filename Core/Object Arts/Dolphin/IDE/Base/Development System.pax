	batchAccessor := KeyedAspectBatchAccessor subject: self
				batchSize: self publishedKeyedAspectsBatchSize.
	keyedAspects := batchAccessor getBatchAccessors collect: [:each | each aspectDescriptor].
	^(LookupTable new)
		addAll: keyedAspects;
		yourself!

