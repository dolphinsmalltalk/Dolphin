	called aString owned by aClass."

	| resID view shell |
	shell := ShellView new create.
	view := shell addSubView: self new.
	(resID := ResourceIdentifier class: aClass name: aStringName) assign: view literalStoreArray.
	shell destroy.
	^resID!

