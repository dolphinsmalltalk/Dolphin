| package |
package := Package name: 'Dolphin Transcript'.
package paxVersion: 1;
	preDeclareClassesOnLoad: false;
	basicComment: ''.

package basicPackageVersion: '6.2'.

package classNames
	add: #TranscriptShell;
	yourself.

package binaryGlobalNames: (Set new
	yourself).

package globalAliases: (Set new
	yourself).

package setPrerequisites: #(
	'..\..\Base\Dolphin'
	'..\..\MVP\Base\Dolphin Basic Geometry'
	'..\..\MVP\Dialogs\Common\Dolphin Common Dialogs'
	'..\..\MVP\Base\Dolphin MVP Base'
	'..\..\MVP\Presenters\Text\Dolphin Text Presenter'
	'..\..\MVP\Type Converters\Dolphin Type Converters').

package!

"Class Definitions"!

Shell subclass: #TranscriptShell
	instanceVariableNames: 'outputWindow buffer bufferProtect'
	classVariableNames: ''
	poolDictionaries: ''
	classInstanceVariableNames: 'DefaultExtent FlashOnOutput'!

"End of package definition"!

