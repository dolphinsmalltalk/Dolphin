	add: #ADODBADORecordConstruction;
	add: #ADODBADORecordsetConstruction;
	add: #ADODBADOStreamConstruction;
	add: #ADODBCommand15;
	add: #ADODBConnection15;
	add: #ADODBError;
	add: #ADODBErrors;
	add: #ADODBField;
	add: #ADODBField15;
	add: #ADODBField20;
	add: #ADODBFields;
	add: #ADODBFields15;
	add: #ADODBFields20;
	add: #ADODBIRecFields;
	add: #ADODBParameters;
	add: #ADODBProperties;
	add: #ADODBProperty;
	add: #ADODBRecordset15;
	add: #ADODBRecordset20;
	add: #ADODBRecordset21;
	add: #ADODynaCollection;
	yourself.

package globalNames
	add: #ADODBConstants;
	add: #ADODBLib;
	yourself.

package binaryGlobalNames: (Set new
	add: #ADODBLib;
	yourself).

package globalAliases: (Set new
	yourself).

package setPrerequisites: (IdentitySet new
	add: '..\..\Automation\ActiveX Automation';
	add: '..\..\..\Base\Dolphin';
	add: '..\..\COM\OLE COM';
	yourself).

package!

