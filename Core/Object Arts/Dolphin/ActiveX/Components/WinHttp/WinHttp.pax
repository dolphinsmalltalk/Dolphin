| package |
package := Package name: 'WinHttp'.
package paxVersion: 1;
	preDeclareClassesOnLoad: false;
	basicComment: 'Dolphin Smalltalk WinHttp Package. 

This package implements a Smalltalk mapping for the Microsoft WinHttp component, which provides a simple and high performance HTTP client.'.

package classNames
	add: #IWinHttpRequest;
	add: #IWinHttpRequestEvents;
	yourself.

package globalNames
	add: #WinHttpConstants;
	add: #WinHttpLib;
	yourself.

package binaryGlobalNames: (Set new
	add: #WinHttpLib;
	yourself).

package globalAliases: (Set new
	yourself).

package setPrerequisites: #(
	'..\..\Automation\ActiveX Automation'
	'..\..\..\Base\Dolphin'
	'..\..\COM\OLE COM').

package!

"Class Definitions"!

IUnknown subclass: #IWinHttpRequestEvents
	instanceVariableNames: ''
	classVariableNames: ''
	poolDictionaries: 'WinHttpConstants'
	classInstanceVariableNames: ''!

IDispatch subclass: #IWinHttpRequest
	instanceVariableNames: ''
	classVariableNames: ''
	poolDictionaries: 'WinHttpConstants'
	classInstanceVariableNames: ''!

"End of package definition"!

