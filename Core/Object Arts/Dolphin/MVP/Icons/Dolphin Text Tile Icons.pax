| package |
package := Package name: 'Dolphin Text Tile Icons'.
package paxVersion: 1;
	preDeclareClassesOnLoad: false;
	basicComment: 'Dolphin Smalltalk Text Tile Icons
Copyright (c) Object Arts Ltd. 2018.
'.

package classNames
	add: #TextTileIcon;
	yourself.

package binaryGlobalNames: (Set new
	yourself).

package globalAliases: (Set new
	yourself).

package setPrerequisites: #(
	'..\..\Base\Dolphin'
	'..\Base\Dolphin Basic Geometry'
	'..\Base\Dolphin MVP Base'
	'..\Gdiplus\Gdiplus').

package!

"Class Definitions"!

Object subclass: #TextTileIcon
	instanceVariableNames: 'text fontName tilecolor textcolor icon _reserved6 flags margin _reserved9'
	classVariableNames: 'FontStyleMask FontStyleNames RotationMask StringFormat'
	poolDictionaries: 'GdiplusConstants'
	classInstanceVariableNames: ''!

"End of package definition"!

