	#{OS.WinSocketConstants}
	#{OS.WinSocketErrors}
	#{OS.WS2_32Library}
	#{OS.WSADATA}
	#{OS.WSAPROTOCOL_INFOW}
	#{OS.WSAPROTOCOLCHAIN}
).

