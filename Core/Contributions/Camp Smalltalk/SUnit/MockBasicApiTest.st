"VERSION:1.0"!TestCase subclass: #MockBasicAPITest	instanceVariableNames: 'mock'	classVariableNames: ''	package: 'SUnit-MockObjects-Tests'!MockBasicAPITest class	instanceVariableNames: ''!!MockBasicAPITest methodsFor: 'running' stamp: '2025-12-23T11:18:12.551+00:00' prior: 0!setUp	super setUp.	mock := MockObject new.	mock		on: #meaningOfLife 		respond: 42. 	mock 		on: #secondMeaning		respond: 84.	! !!MockBasicAPITest methodsFor: 'running' stamp: '2025-12-23T11:18:12.559+00:00' prior: 0!testMeaningOfLife	<ignoreNotImplementedSelectors: #(meaningOfLife secondMeaning)>		self assert: mock meaningOfLife equals: 42.	"We verify that all the messages have been consumed. 	Since this is not the case we raise an error."	self should: [ self verify: mock ] raise: TestFailure.	self assert: mock secondMeaning equals: 84! !!MockBasicAPITest methodsFor: 'running' stamp: '2025-12-23T11:18:12.564+00:00' prior: 0!testMeaningOfLifeIsFirst	<ignoreNotImplementedSelectors: #( meaningOfLife secondMeaning )>	self assert: mock meaningOfLife equals: 42.	self assert: mock secondMeaning equals: 84.		"We verify that all the messages have been consumed. 	Since this is the case we did not get an error."	self shouldnt: [ self verify: mock ] raise: TestFailure.! !!MockBasicAPITest methodsFor: 'running' stamp: '2025-12-23T11:18:12.569+00:00' prior: 0!testMeaningOfLifeIsSentOnce	"A simple mock object can answer a message only once and in the same order."	<ignoreNotImplementedSelectors: #( meaningOfLife )>	self assert: mock meaningOfLife equals: 42! !!MockBasicAPITest methodsFor: 'running' stamp: '2025-12-23T11:18:12.571+00:00' prior: 0!testMeaningOfLifeIsSentTwice	<ignoreNotImplementedSelectors: #( meaningOfLife )>	mock meaningOfLife.	self should: [ self assert: mock meaningOfLife equals: 42 ] raise: TestFailure! !!MockBasicAPITest methodsFor: 'running' stamp: '2025-12-23T11:18:12.575+00:00' prior: 0!testVerifyChecksThatAllTheMessageGotSent	<ignoreNotImplementedSelectors: #( meaningOfLife secondMeaning )>	self assert: mock meaningOfLife equals: 42.	self assert: mock secondMeaning equals: 84.	self verify: mock! !!MockBasicAPITest methodsFor: 'running' stamp: '2025-12-23T11:18:12.578+00:00' prior: 0!testVerifyFailsWhenMessageShouldBeSent	<ignoreNotImplementedSelectors: #( meaningOfLife )>	self assert: mock meaningOfLife equals: 42.	self should: [ self verify: mock ] raise: TestFailure! !