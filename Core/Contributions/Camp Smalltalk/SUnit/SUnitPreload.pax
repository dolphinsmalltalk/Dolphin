| package |
package := Package name: 'SUnitPreload'.
package paxVersion: 1;
	preDeclareClassesOnLoad: false;
	basicComment: ''.

package classNames
	add: #ResumableTestFailure;
	add: #SUnitNameResolver;
	add: #TestFailure;
	yourself.

package methodNames
	add: #Exception -> #sunitExitWith:;
	yourself.

package globalNames
	add: #SUnitDelay;
	yourself.

package binaryGlobalNames: (Set new
	yourself).

package globalAliases: (Set new
	add: #SUnitDelay;
	yourself).

package setPrerequisites: #(
	'..\..\..\Object Arts\Dolphin\Base\Dolphin').

package!

"Class Definitions"!

Object subclass: #SUnitNameResolver
	instanceVariableNames: ''
	classVariableNames: ''
	poolDictionaries: ''
	classInstanceVariableNames: ''!

Error subclass: #TestFailure
	instanceVariableNames: ''
	classVariableNames: ''
	poolDictionaries: ''
	classInstanceVariableNames: ''!

TestFailure subclass: #ResumableTestFailure
	instanceVariableNames: ''
	classVariableNames: ''
	poolDictionaries: ''
	classInstanceVariableNames: ''!

"Global Aliases"!

SUnitDelay := Delay!


"Loose Methods"!

!Exception methodsFor!

sunitExitWith: aValue
	self return: aValue! !

!Exception categoriesForMethods!
sunitExitWith:!Camp Smalltalk!handler responses!public! !
!

"End of package definition"!

