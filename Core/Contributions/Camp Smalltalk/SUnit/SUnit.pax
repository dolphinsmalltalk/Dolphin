| package |
package := Package name: 'SUnit'.
package paxVersion: 1;
	basicComment: ''.


package classNames
	add: #TestCase;
	add: #TestCaseResult;
	add: #TestResource;
	add: #TestResult;
	add: #TestSuite;
	yourself.

package binaryGlobalNames: (Set new
	yourself).

package globalAliases: (Set new
	yourself).

package setPrerequisites: (IdentitySet new
	add: '..\..\..\Object Arts\Dolphin\Base\Dolphin';
	add: 'SUnitPreload';
	yourself).

package!

